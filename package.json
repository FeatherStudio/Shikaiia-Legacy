{
  "name": "Shikkaiia",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "tsc && jest --no-cache --coverage",
    "jest": "jest --no-cache --coverage"
  },
  "dependencies": {
    "@types/node": "^7.0.5",
    "@types/typescript": "^2.0.0",
    "body-parser": "~1.15.1",
    "cookie-parser": "~1.4.3",
    "socket.io": "^1.4.8"
  },
  "devDependencies": {
    "@types/jest": "^16.0.1",
    "jest": "^17.0.3",
    "shx": "^0.2.1",
    "ts-jest": "^17.0.3",
    "typescript": "^2.0.10"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "json",
      "js"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|.*\\.(test|spec))\\.(ts)$",
    "verbose": true,
    "testResultsProcessor": "<rootDir>/node_modules/ts-jest/coverageprocessor.js"
  }
}
